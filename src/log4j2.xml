<?xml version="1.0" encoding="UTF-8"?>

<!-- https://blog.csdn.net/dgwdfkkcj01/article/details/72855674 -->
<Configuration status="warn">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%m%n %t" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>